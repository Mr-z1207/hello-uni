{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				// 页面样式属性，会覆盖全局样式属性
				// ...
			}
		},
		{
			"path": "pages/toDoList/toDoList",
			"style": {
				"navigationBarTitleText": "toDoList"
			}
		},
		{
			"path": "pages/blog/blog",
			"style": {
				"navigationBarTitleText": "blog"
			}
		}
	],
	// 全局样式属性
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-Dome",
		"navigationBarBackgroundColor": "#7AC5CD",
		// 背景颜色，下拉刷新时后面的颜色，默认看不到，所以要开启下拉
		"backgroundColor": "#f7f7f7",
		"enablePullDownRefresh": true  // 开启下拉刷新
	},
	"tabBar":{
		"color": "#333333",
		"selectedColor": "#FF6347",
		"backgroundColor": "#7AC5CD",
		"list":[
			{
				"pagePath": "pages/blog/blog",
				"text": "博客",
				"iconPath": "static/taberIcon/blog.png",
				"selectedIconPath": "static/taberIcon/blog-active.png"
			},
			{
				"pagePath": "pages/index/index",
				"text": "主页",
				"iconPath": "static/taberIcon/home.png",
				"selectedIconPath": "static/taberIcon/home-active.png"
			},
			{
				"pagePath": "pages/toDoList/toDoList",
				"text": "待办",
				"iconPath": "static/taberIcon/list.png",
				"selectedIconPath":"static/taberIcon/list-active.png"
			}
		]
	}
}
